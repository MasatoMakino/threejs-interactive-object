{
  "name": "threejs-interactive-object",
  "version": "0.12.2",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "repository": "https://MasatoMakino@github.com/MasatoMakino/threejs-interactive-object.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "files": [
    "lib",
    "esm"
  ],
  "keywords": [
    "threejs"
  ],
  "peerDependencies": {
    "three": ">=0.126.0 <1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/three": "^0.139.0",
    "canvas": "^2.9.1",
    "fake-mouse-event": "github:MasatoMakino/fake-mouse-event",
    "gl": "^5.0.0",
    "gulptask-demo-page": "github:MasatoMakino/gulptask-demo-page",
    "gulptask-dev-server": "github:MasatoMakino/gulptask-dev-server",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "three": "^0.139.2",
    "ts-jest": "^27.1.4",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "coverage": "jest --coverage",
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "build": "npm run demo & npm run buildTS",
    "demo": "npx gulptask-demo-page --body '<canvas id=\"webgl-canvas\"></canvas>'",
    "buildTS": "npx tsc --project tsconfig.cjs.json & tsc --project tsconfig.esm.json",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts",
    "server": "npx gulptask-dev-server -b ./docs/demo",
    "watch:demo": "npm run demo -- -W",
    "watch:tsc": "tsc -W --incremental --project tsconfig.cjs.json & tsc -W --incremental --project tsconfig.esm.json"
  },
  "dependencies": {
    "raf-ticker": "github:MasatoMakino/raf-ticker"
  }
}
