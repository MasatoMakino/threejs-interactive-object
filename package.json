{
  "name": "threejs-interactive-object",
  "version": "0.7.3",
  "main": "./bin/index.js",
  "types": "./bin/index.d.ts",
  "repository": "https://MasatoMakino@github.com/MasatoMakino/threejs-interactive-object.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "keywords": [
    "threejs"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@types/jest": "^24.0.0",
    "babel-loader": "^8.0.4",
    "http-server": "^0.11.1",
    "husky": "^2.3.0",
    "jest": "^24.1.0",
    "prettier": "^1.15.2",
    "replace-in-file": "^4.1.0",
    "three": "^0.104.0",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "npm run prettier:src && tsc && npm run babel && npm run doc",
    "prettier:src": "prettier --write './!(bin|docs|coverage)/**/*.{js,ts,json}' ",
    "server": "http-server ./docs/ -o",
    "babel": "webpack --mode development --module-bind js=babel-loader ./docs/main.js --output ./docs/bundle.js",
    "doc": "npm run doc:generate && npm run doc:replaceNode && npm run doc:card",
    "doc:generate": "typedoc --tsconfig tsdocconfig.json src",
    "doc:replaceNode": "node ./task/replaceAPI.js",
    "doc:card": "repoimg -n MasatoMakino/threejs-interactive-object -o ./docs/card.png"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:src"
    }
  }
}
