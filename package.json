{
  "name": "threejs-interactive-object",
  "version": "0.7.1",
  "main": "./bin/index.js",
  "types": "./bin/index.d.ts",
  "repository": "https://MasatoMakino@github.com/MasatoMakino/threejs-interactive-object.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "keywords": [
    "threejs"
  ],
  "dependencies": {
    "@types/three": "^0.93.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@types/jest": "^23.3.7",
    "babel-loader": "^8.0.4",
    "cpx": "^1.5.0",
    "http-server": "^0.11.1",
    "husky": "^1.1.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "three": "^0.97.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "npm run prettier:src && tsc && npm run copy && npm run remove  && npm run babel",
    "copy": "cpx './bin/src/*' ./bin",
    "remove": "rimraf ./bin/__test__ ./bin/src",
    "prettier:src": "prettier --write './!(bin)/**/*.{js,ts,json}' ",
    "server": "http-server ./docs/ -o",
    "babel": "webpack --mode development --module-bind js=babel-loader ./docs/main.js --output ./docs/bundle.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build"
    }
  }
}
