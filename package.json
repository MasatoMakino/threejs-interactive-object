{
  "name": "@masatomakino/threejs-interactive-object",
  "version": "0.14.2",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://MasatoMakino@github.com/MasatoMakino/threejs-interactive-object.git"
  },
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "files": [
    "lib",
    "esm"
  ],
  "keywords": [
    "threejs"
  ],
  "dependencies": {
    "@masatomakino/raf-ticker": "^0.4.2"
  },
  "devDependencies": {
    "@masatomakino/fake-mouse-event": "^0.0.10",
    "@masatomakino/gulptask-demo-page": "^0.6.5",
    "@swc/core": "^1.3.50",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.1.0",
    "@types/three": "^0.152.0",
    "browser-sync": "^2.29.1",
    "canvas": "^2.11.2",
    "gl": "^6.0.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "three": "^0.152.2",
    "typedoc": "^0.24.1",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "three": ">=0.126.0 <1.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "coverage": "jest --coverage",
    "preversion": "git checkout main && git pull && npm run test",
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "build": "npm run buildTS && npm run demo && npm run typedocs",
    "demo": "npx @masatomakino/gulptask-demo-page --body '<canvas id=\"webgl-canvas\"></canvas>'",
    "buildTS": "tsc --project tsconfig.cjs.json & tsc --project tsconfig.esm.json",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts",
    "server": "browser-sync ./docs/demo -w",
    "watch:demo": "npm run demo -- -W",
    "watch:tsc": "tsc -W --incremental --project tsconfig.cjs.json & tsc -W --incremental --project tsconfig.esm.json"
  },
  "description": "Mouse interactive objects for three.js",
  "bugs": {
    "url": "https://github.com/MasatoMakino/threejs-interactive-object/issues"
  },
  "homepage": "https://github.com/MasatoMakino/threejs-interactive-object#readme",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
